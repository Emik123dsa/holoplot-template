{
  "npmScope": "holoplot.schema",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "cdk": {
      "tags": []
    },
    "common": {
      "tags": []
    },
    "core": {
      "tags": []
    },
    "design": {
      "tags": []
    },
    "vnd.github+json": {
      "tags": []
    },
    "vnd.github+json-e2e": {
      "tags": [],
      "implicitDependencies": ["vnd.github+json"]
    },
    "schema": {
      "tags": []
    },
    "server": {
      "tags": []
    }
  }
}
