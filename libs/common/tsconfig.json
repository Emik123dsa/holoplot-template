{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "webpack-env",
      "node"
    ],
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@holoplot/core/*": [
        "../core/src/lib/*"
      ],
      "@holoplot/common/*": [
        "../common/src/lib/*"
      ],
      "@holoplot/common/http/*": [
        "../common/http/*"
      ],
      "@holoplot/common/http": [
        "../common/http/index.ts"
      ],
      "@holoplot/api/*": [
        "../api/src/lib/*"
      ],
    }
  },
  "include": [],
  "files": [],
  "references": [
    {
      "path": "./tsconfig.lib.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ]
}